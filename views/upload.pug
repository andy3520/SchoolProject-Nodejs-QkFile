section#upload.contactus.upload-section
  .container
    .row
      .col-md-12
        .section-title
          h2.font-weight.color-y.font-weight-h.text-transform.transform-color Upload File
  .section.contact-us
    .container
      .outer-box.text-center
        // Contact Form Start
        .form-box.clearfix
          form(action="/guest/upload" method="post" enctype="multipart/form-data")#uploadform.shake.scroll-reveal(data-toggle='validator' )
            .form-group.col-12
              .file-upload
                .file-select
                  #fileName.file-select-button Choose File
                  #noFile.file-select-name No file chosen...
                  input#chooseFile(type='file' name='file')
            .row
              .form-group.col-xs-12.col-md-6.col-xs-offset-0.col-md-offset-3
                input#findpass.form-control.input-code(type='password', placeholder='Password(optional)' name='pass')
              .col-xs-12
                input(type="submit" value="Upload").btn.upload-btn
                //a.btn.upload-btn(href='#') Upload
                #msgSubmit