section#findfile.contactus.find-file-section
  .container
    .row
      .col-md-12
        .section-title
          h2.font-weight.color-y.font-weight-h.text-transform Find File
  .section.contact-us
    .container-fluid
      .row
        .col-xs-10.col-xs-offset-1.col-sm-8.col-sm-offset-2.col-md-6.col-md-offset-3
          .outer-box.text-center
            // Contact Form Start
            .form-box.clearfix
              form#findform.shake.scroll-reveal(data-toggle='validator' action='/guest/find' method='post')
                .form-group.col-xs-12
                  input#findcode-find.border-color-y.form-control.margin-bottom-1.input-code(type='text', placeholder='Enter File Code' name='code')
                  input#pass-find.border-color-y.form-control.input-code(type='password', placeholder='Enter password if exist' name='pass')
                .col-xs-12
                  input(type="submit" value="Find").btn.upload-btn
                  //a.btn(href='#') Find
                  //#msgSubmit
.clearfix